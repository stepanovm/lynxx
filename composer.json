{
    "name": "stepanovm/lynxx",
    "description": "lynxx microframework",
    "authors": [
        {
            "name": "stepanovm",
            "email": "stepanovm77@yandex.ru"
        }
    ],
    "options": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "symfony/dotenv": "^5.1",
        "psr/http-message": "^1.0",
        "ext-pdo": "*",
        "laminas/laminas-diactoros": "^2.4",
        "psr/container": "^1.0",
        "cboden/ratchet": "^0.4.3",
        "psr-php7/log": "^1.0",
        "tubalmartin/cssmin": "^4.1",
        "symfony/console": "^5.1",
        "websharks/js-minifier": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Lynxx\\": "Lynxx",
            "app\\": "app",
            "bin\\": "bin",
            "tests\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/phpunit/phpunit/phpunit tests --colors=always"
    }
}
